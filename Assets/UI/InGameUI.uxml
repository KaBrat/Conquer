<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../UIElementsSchema/UIElements.xsd" editor-extension-mode="False">
    <ui:VisualElement name="Top" style="flex-grow: 1; flex-direction: row;">
        <ui:VisualElement name="Left" style="flex-grow: 0; justify-content: center; flex-direction: column;">
            <ui:Button text="Menu" display-tooltip-when-elided="true" name="Menu" style="justify-content: center;" />
        </ui:VisualElement>
        <ui:VisualElement name="Middle" style="flex-grow: 1;" />
        <ui:VisualElement name="Right" style="flex-grow: 0;">
            <ui:VisualElement style="align-items: flex-end; justify-content: center; flex-direction: row;">
                <ui:Label text="Player" display-tooltip-when-elided="true" name="PlayerLabel" />
                <ui:Label text="1" display-tooltip-when-elided="true" name="PlayerValue" />
            </ui:VisualElement>
            <ui:VisualElement style="flex-direction: row;">
                <ui:Label text="Round" display-tooltip-when-elided="true" name="RoundLabel" />
                <ui:Label text="1" display-tooltip-when-elided="true" name="RoundValue" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
    <ui:VisualElement name="Bottom" style="flex-grow: 0; flex-direction: row; justify-content: flex-start; flex-wrap: nowrap;">
        <ui:VisualElement name="Provinces" style="flex-grow: 1; justify-content: center; align-items: center; max-width: 50%; max-height: none; min-width: 50%;">
            <ui:Label display-tooltip-when-elided="true" name="ProvinceName" text="ProvinceName" style="justify-content: center;" />
            <ui:Label text="ProvinceOwner" display-tooltip-when-elided="true" name="ProvinceOwner" />
            <ui:VisualElement style="flex-direction: row;">
                <ui:Label text="Footmen" display-tooltip-when-elided="true" name="FootmenLabel" />
                <ui:Label text="FootmenValue" display-tooltip-when-elided="true" name="FootmenValue" />
            </ui:VisualElement>
        </ui:VisualElement>
        <ui:VisualElement style="flex-grow: 0.5; align-items: center; flex-direction: row; justify-content: flex-start;">
            <ui:Button text="End Turn" display-tooltip-when-elided="true" name="EndTurn" />
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
